import math

def calculate_min(a,b,c,d,n):
    
    result = float('inf')
    
    # results_dict = {}
    for i in range(1, n):
        
        derivative = 2 * a * (i**2) + 2 * b + c 
        
        # results_dict[i] = derivative
        if derivative <= result:

            
            result = derivative
            num = i 
            
            

    return num
